@page
@model Project_Market.Areas.Identity.Pages.Account.Manage.UploadPhotosModel
@{
    ViewData["Title"] = "My photos";
    ViewData["ActivePage"] = ManageNavPages.UploadPhotos;
}

<h4>@ViewData["Title"]</h4>
<div class="row">
    <div class="col container">
        <div class="col-10">
            @foreach (var items in Model.ImageList)
            {
                var iPhotoUrl = "/images/" + items;
            <img class="float-left p-2" src="@iPhotoUrl" height="150" />

            }
        </div>
    </div>
</div>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}